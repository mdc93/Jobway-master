<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<fx:root fx:id="borderPaneContent" prefHeight="993.0" prefWidth="1185.0" styleClass="mainWindow" type="BorderPane" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <VBox id="VBoxmenu" alignment="BOTTOM_CENTER" minWidth="45.0" prefWidth="45.0" BorderPane.alignment="CENTER">
         <children>
            <Region prefHeight="278.0" prefWidth="45.0" />
            <Button fx:id="buttonDisplay" ellipsisString="" mnemonicParsing="false" onAction="#toMenuPerson" prefHeight="44.0" prefWidth="73.0" styleClass="button-menu" text="Bénéficiaires" textOverrun="WORD_ELLIPSIS">
               <graphic>
                  <FontIcon iconLiteral="far-address-card" iconSize="20" />
               </graphic>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="buttonUsers" disable="true" ellipsisString="" mnemonicParsing="false" onAction="#toUsers" prefHeight="44.0" prefWidth="51.0" styleClass="button-menu" text="Utilisateurs" textAlignment="CENTER" textOverrun="WORD_ELLIPSIS">
               <graphic>
                  <FontIcon iconLiteral="far-user" iconSize="20" />
               </graphic>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
            <Button fx:id="buttonGestion" disable="true" ellipsisString="" mnemonicParsing="false" onAction="#toGestion" prefHeight="44.0" prefWidth="200.0" styleClass="button-menu" text="Gestion" textOverrun="WORD_ELLIPSIS">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
               <graphic>
                  <FontIcon iconLiteral="bi-laptop-fill" iconSize="20" />
               </graphic>
               <font>
                  <Font size="11.0" />
               </font>
            </Button>
            <Region prefHeight="323.0" prefWidth="45.0" VBox.vgrow="ALWAYS" />
            <Button fx:id="buttonClose" ellipsisString="" mnemonicParsing="false" onAction="#onCloseButtonClicked" prefHeight="38.0" prefWidth="73.0" styleClass="button-menu" text="Quitter" textOverrun="WORD_ELLIPSIS">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <graphic>
                  <FontIcon iconLiteral="bi-power" iconSize="20" />
               </graphic>
            </Button>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </VBox>
   </left>
   <top>
      <HBox prefHeight="150.0" prefWidth="1185.0" BorderPane.alignment="CENTER">
         <children>
            <VBox prefHeight="150.0" prefWidth="252.0" HBox.hgrow="ALWAYS">
               <children>
                  <Region prefHeight="200.0" prefWidth="4.0" styleClass="region-header1" />
                  <Region prefHeight="218.0" prefWidth="335.0" styleClass="region-header2" />
               </children>
            </VBox>
            <HBox prefHeight="150.0" prefWidth="667.0" styleClass="hbox-header" HBox.hgrow="NEVER">
               <children>
                  <VBox />
               </children>
            </HBox>
            <VBox alignment="TOP_RIGHT" nodeOrientation="LEFT_TO_RIGHT" prefHeight="150.0" prefWidth="0.0" HBox.hgrow="ALWAYS">
               <children>
                  <HBox alignment="TOP_RIGHT" prefHeight="82.0" prefWidth="283.0" styleClass="region-header1" VBox.vgrow="ALWAYS">
                     <children>
                        <Label fx:id="labelUserName" contentDisplay="CENTER" prefHeight="45.0" prefWidth="113.0" styleClass="region-header1" text="Admin (Administrateur)" wrapText="true">
                           <font>
                              <Font name="System Bold" size="13.0" />
                           </font>
                        </Label>
                        <Button ellipsisString="" mnemonicParsing="false" onAction="#changeStyleSheet" prefHeight="45.0" prefWidth="45.0" styleClass="region-header1" textOverrun="WORD_ELLIPSIS">
                           <graphic>
                              <FontIcon id="darkMode" iconSize="20" />
                           </graphic>
                        </Button>
                     </children>
                  </HBox>
                  <Region prefHeight="58.0" prefWidth="355.0" styleClass="region-header1" VBox.vgrow="ALWAYS" />
                  <Region prefHeight="121.0" prefWidth="283.0" styleClass="region-header2" />
               </children>
            </VBox>
         </children>
      </HBox>
   </top>
</fx:root>
